{
    "version": "1.0",
    "defaultSchema": "testdb",
    "schemas": [
        {
            "name": "testdb",
            "type": "custom",
            "factory": "cli.Apache.Calcite.Adapter.Ado.AdoSchemaFactory",
            "operand": {
                "adoProviderName": "Microsoft.Data.Sqlite",
                "adoConnectionString": "Data Source=./test.db",
                "adoDatabaseMetadata": "Apache.Calcite.Adapter.Ado.Metadata.SqliteDatabaseMetadata",
                "adoSchema": ""
            }
        },
        {
            "name": "csv",
            "type": "custom",
            "factory": "org.apache.calcite.adapter.csv.CsvSchemaFactory",
            "operand": {
                "directory": "D:\\calcite-adonet\\src\\Apache.Calcite.Adapter.Ado.Tests\\csv\\",
                "flavor": "translatable"
            }
        },
        {
            "name": "adhoc",
            "tables": [
                {
                    "name": "people",
                    "type": "view",
                    "sql": "SELECT * FROM \"testdb\".\"table1\" T1 INNER JOIN \"csv\".\"people\" P on P.\"Id\" = T1.\"Id\""
                }
            ]
        }
    ]
}
